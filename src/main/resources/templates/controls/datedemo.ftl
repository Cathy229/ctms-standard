<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
</head>
<body>
<div class="container-fluid">
	<div class="row">
		<div class="col-md-12">
			<button id="updateBtn" type="button" class="btn btn-yellow">设置值</button>
		</div>
	</div>
	<div class="row">
		<form id="form">
		<div class="col-md-12 form-horizontal">
			<div class="col-md-4">
				<div class="form-group">
					<label class="col-md-4">日期</label>
					<div class="col-md-8">
						<div class="input-group">
							<input type="text" class="form-control" id="intInput" name="intInput">
							<div class="input-group-addon" role="button">
								<i class="icon-calendar"></i>
							</div>
						</div>
					</div>
				</div>
			</div>	
			<div class="col-md-4">
				<div class="form-group">
					<label class="col-md-4">日期时间</label>
					<div class="col-md-8">
						<div class="input-group">
							<input type="text" class="form-control" id="intInput2" name="intInput2">
							<div class="input-group-addon" role="button">
								<i class="icon-calendar"></i>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		</form>
	</div>
</div>
</body>
</html>
